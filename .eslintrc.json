{
    "extends": ["airbnb", "prettier"],
    "plugins": ["prettier"],
    "rules": {
        "prettier/prettier": [
            "warn",
            {
                "endOfLine": "auto"
            }
        ],
        "no-unused-vars": "warn",
        "no-undef": "off",
        "class-methods-use-this": "off",
        "arrow-body-style": ["error", "as-needed"],
        "prefer-arrow-callback": "off",
        "func-names": "off",
        "no-console": "warn"
    }
}
