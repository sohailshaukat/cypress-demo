{
  "author": "Sohail Shaukat",
  "scripts": {
    "cy:open": "cypress open",
    "cy:run": "cypress run --record false",
    "cy:record": "cypress run --record",
    "cy:chrome": "npx cypress run --browser chrome --record false",
    "cy:firefox": "npx cypress run --browser firefox --record false",
    "cy:chrome:record": "npx cypress run --browser chrome --record",
    "cy:firefox:record": "npx cypress run --browser firefox --record",
    "cy:chrome:headed": "npx cypress run --browser chrome --headed --record false",
    "cy:firefox:headed": "npx cypress run --browser firefox --headed --record false",
    "cy:chrome:headed:record": "npx cypress run --browser chrome --record --headed",
    "cy:firefox:headed:record": "npx cypress run --browser firefox --record --headed",
    "cy:spec:product": "npx cypress run --spec 'cypress/plotly/tests/product.spec.js' --record false",
    "cy:spec:default": "npx cypress run --spec 'cypress/plotly/tests/default.spec.js' --record false"
  },
  "dependencies": {
    "cypress": "^13.6.2"
  },
  "devDependencies": {
    "eslint": "^7.26.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-node": "^4.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "prettier": "^2.3.0"
  }
}
